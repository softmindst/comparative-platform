{
    "version": 2,
    "builds": [
      {
        "src": "dist/panel/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "redirects": [],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Encoding", "value": "gzip" },
          { "key": "Pragma", "value": "no-cache" },
          { "key": "Expires", "value": "0" }
          
        ]
      }
    ]
  }
  